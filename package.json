{
  "name": "fandem-web",
  "version": "0.1.0",
  "description": "fandem.io! Built with React, GraphQL, Next, Apollo and lot's of love",
  "repository": "https://github.com/MikeAlexMartinez/fandem-web",
  "license": "MIT",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start": "next start",
    "test": "NODE_ENV=test && jest --watch",
    "e2e": "NODE_ENV=e2e && cucumber-js e2e/features/**/*.feature --require-module @babel/register --require e2e/steps/**/*.steps.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "apollo-boost": "^0.1.16",
    "apollo-client": "^2.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-styled-components": "^1.7.1",
    "date-fns": "^2.0.0-alpha.7",
    "downshift": "^2.2.3",
    "email-validator": "^2.0.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "jss": "^9.8.7",
    "lodash.debounce": "^4.0.8",
    "next": "^7.0.0",
    "next-with-apollo": "^3.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.2.1",
    "react-dom": "^16.5.2",
    "react-jss": "^8.6.1",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^2.5.0"
  },
  "devDependencies": {
    "@babel/register": "^7.4.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "enzyme-to-json": "^3.3.4",
    "graphql-tools": "^4.0.0",
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "puppeteer": "^1.13.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "inline-react-svg"
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": []
      },
      "test": {
        "presets": [],
        "plugins": []
      }
    }
  }
}
